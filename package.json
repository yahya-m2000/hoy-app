{
  "name": "mobile_app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "clear": "npx expo start --clear",
    "build:production": "node scripts/production-build.js",
    "build:validate": "node scripts/production-build.js validate",
    "build:check": "node scripts/production-build.js check",
    "build:config": "node scripts/production-build.js config",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:all": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "security:check": "npm audit --production",
    "test:certificate-pinning": "jest src/core/security/__tests__/certificate-pinning.test.ts",
    "env:setup": "cp .env.example .env && echo 'Please update .env with your values'",
    "env:validate": "node scripts/validate-env.js",
    "clean:cache": "npx expo start --clear && watchman watch-del-all && rm -rf node_modules package-lock.json && npm install",
    "prebuild:ios": "npx expo prebuild --platform ios && npx expo run:ios",
    "prebuild:ios:device": "npx expo prebuild --platform ios && npx expo run:ios --device $npm_config_udid",
    "prebuild:android:device": "npx expo prebuild --platform android && npx expo run:android --device $npm_config_udid",
    "prebuild:android": "npx expo prebuild --platform android && npx expo run:android",
    "prebuild": "npm run prebuild:ios && npm run prebuild:android",
    "fix:pods": "arch -arm64 gem uninstall ffi -aIx || true && arch -arm64 gem install ffi && arch -arm64 gem uninstall cocoapods -aIx || true && arch -arm64 gem install cocoapods && cd ios && arch -arm64 pod install"
  },
  "dependencies": {
    "@expo/metro-runtime": "^5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.1.6",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@rneui/base": "4.0.0-rc.8",
    "@tanstack/react-query": "^5.75.1",
    "axios": "^1.9.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "expo": "53.0.19",
    "expo-apple-authentication": "~7.2.4",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.10",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-contacts": "^14.2.5",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "^5.2.4",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.2",
