{
  "name": "mobile_app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "clear": "npx expo start --clear",
    "build:production": "node scripts/production-build.js",
    "build:validate": "node scripts/production-build.js validate",
    "build:check": "node scripts/production-build.js check",
    "build:config": "node scripts/production-build.js config",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:all": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "security:check": "npm audit --production",
    "test:certificate-pinning": "jest src/core/security/__tests__/certificate-pinning.test.ts",
    "env:setup": "cp .env.example .env && echo 'Please update .env with your values'",
    "env:validate": "node scripts/validate-env.js",
    "clean:cache": "npx expo start --clear && watchman watch-del-all && rm -rf node_modules && npm install"
  },
  "dependencies": {
