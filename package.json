    "expo-location": "~18.1.6",
    "expo-maps": "~0.11.0",
    "expo-media-library": "~17.1.7",
    "expo-network": "~7.1.5",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.3",
    "expo-secure-store": "~14.2.3",
    "expo-sensors": "~14.1.4",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.2.0",
    "i18next": "^25.0.2",
    "jose": "^6.0.11",
    "jwt-decode": "^4.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.5.1",
    "react-native": "0.79.5",
    "react-native-auth0": "^5.0.0-beta.3",
